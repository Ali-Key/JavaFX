<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="277.0" prefWidth="797.0" style="-fx-background-color: #696666;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.assigmentlap.Assigmentlap">
   <children>
      <Label fx:id="songLabel" layoutX="213.0" layoutY="106.0" prefHeight="55.0" prefWidth="358.0" text="112 - Al-lklas.mp3" textFill="WHITE">
         <font>
            <Font size="43.0" />
         </font>
      </Label>
      <Button fx:id="playButton" layoutX="24.0" layoutY="243.0" mnemonicParsing="false" onAction="#playMedia" prefHeight="58.0" prefWidth="94.0" text="Play" textFill="#f4f4f4">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <Button fx:id="pauseButton" layoutX="136.0" layoutY="243.0" mnemonicParsing="false" onAction="#pauseMedia" prefHeight="58.0" prefWidth="116.0" text="Pause">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <Button fx:id="previousButton" layoutX="416.0" layoutY="243.0" mnemonicParsing="false" onAction="#previousMedia" prefHeight="58.0" prefWidth="103.0" text="Prev.">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <Button fx:id="nextButton" layoutX="537.0" layoutY="243.0" mnemonicParsing="false" onAction="#nextMedia" prefHeight="58.0" prefWidth="103.0" text="Next">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <Button fx:id="resetButton" layoutX="263.0" layoutY="243.0" mnemonicParsing="false" onAction="#resetMedia" prefHeight="58.0" prefWidth="137.0" text="Reset">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <Slider fx:id="volumeSlider" layoutX="730.0" layoutY="49.0" onDragDetected="#voidchangeSpeed" orientation="VERTICAL" prefHeight="271.0" prefWidth="59.0" value="50.0" />
      <ComboBox fx:id="speedBox" layoutX="656.0" layoutY="243.0" prefHeight="58.0" prefWidth="74.0" promptText="speed" />
      <ProgressBar fx:id="songProgressBar" layoutX="18.0" layoutY="207.0" prefHeight="20.0" prefWidth="714.0" progress="0.93" />
   </children>
</AnchorPane>
